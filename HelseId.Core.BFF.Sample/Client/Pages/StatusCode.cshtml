@page
@model StatusCodeModel

@switch (Model.ErrorStatusCode)
{
    case 404:
        <partial name="NotFound" model="null"/>
        break;
    case 403:
        <partial name="Forbidden" model="null"/>
        break;
    default:
        <h1>Oops. Noe gikk galt.</h1>
        <p>Det oppsto en feil ved visning av siden. Prøv å gå til <a href="/">forsiden</a>.</p>
        break;
}